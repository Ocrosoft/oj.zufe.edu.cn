#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    long n=100,a,b,c;
    while(n<1000)
    {
        c=n%10;//个位
        b=(n/10)%10;
        a=n/100;//百位
        if (n==(pow(a,3)+pow(b,3)+pow(c,3)))
        {
            printf("%ld\n",n);
        }
        n++;
    }
    return 0;
}
